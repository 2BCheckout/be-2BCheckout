{
  "name": "Ride",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "routeId": {
      "type": "number"
    },
    "plate": {
      "type": "string"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "latitude": {
      "type": "number"
    },
    "longitude": {
      "type": "number"
    },
    "isActive": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "students": {
      "type": "hasMany",
      "model": "Student",
      "foreignKey": "rideId",
      "through": "Boarding",
      "keyThrough": "studentId"
    },
    "route": {
      "type": "belongsTo",
      "model": "Route",
      "foreignKey": "id"
    }
  },
  "acls": [],
  "methods": {}
}
